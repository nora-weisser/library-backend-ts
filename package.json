{
  "name": "library-management-backend",
  "version": "1.0.0",
  "description": "Library management system backend with Express and TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "test:e2e": "mocha integration_tests/**/*.ts",
    "test:e2e:report": "mocha integration_tests/**/*.ts --reporter mochawesome",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn src/server.ts",
    "test:unit": "jest --coverage"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "dotenv": "^17.2.3",
    "jest": "^29.7.0",
    "mocha": "^11.7.5",
    "mochawesome": "^7.1.4",
    "pactum": "^3.8.0",
    "ts-jest": "^29.4.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
